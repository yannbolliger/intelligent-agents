<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->

	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="bollger.ReactiveRandomAgent"/>
		<set discount-factor=".85"/>
	</agent>

	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="bollger.ReactiveRLAAgent"/>
		<set discount-factor=".85"/>
	</agent>

	<agent name="reactive-dummy">
		<set class-path="bin/"/>
		<set class-name="bollger.ReactiveDummyAgent"/>
		<set discount-factor="0.3"/>
	</agent>
</agents>
